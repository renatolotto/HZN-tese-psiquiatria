SELECT *, FROM data d
inner join escavador e on d.id = e.id_cnes
inner join receita r on r.cnpj = d.CNPJ
where ((leitos_saude_mental + leitos_hospital_dia + leitos_psiquiatria + leitos_neurologia + leitos_geriatria + leitos_geriatria_dia)  > 0 or ("MEDICO NEUROLOGISTA" + "MEDICO PSIQUIATRA" + "MEDICO NEUROFISIOLOGISTA CLINICO" + "PSICOLOGO EDUCACIONAL" + "PSICOLOGO CLINICO" + "PSICOLOGO HOSPITALAR" + "PSICOLOGO SOCIAL" + "PSICOLOGO DO TRABALHO" + "NEUROPSICOLOGO" + "PSICANALISTA" + "PSICOLOGO ACUPUNTURISTA") > 0) and "Natureza JurÃ­dica" in (2,3)